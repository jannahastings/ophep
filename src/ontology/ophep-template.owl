    <!-- http://purl.obolibrary.org/obo/OPHEP_000001 -->

    <owl:Class rdf:about="http://purl.obolibrary.org/obo/OPHEP_000001">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/MFOMD_0000024"/>
        <rdfs:subClassOf>
